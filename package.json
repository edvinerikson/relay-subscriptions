{
  "name": "relay-subscriptions",
  "version": "0.3.0",
  "description": "Realtime utils for Relay",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src --ignore __tests__ --out-dir lib",
    "watch": "npm run build -- --watch",
    "test": "npm run lint && npm run check && npm run testonly",
    "lint": "eslint examples src",
    "check": "flow check src",
    "testonly": "jest",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edvinerikson/relay-subscriptions.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "relay",
    "react",
    "subscriptions"
  ],
  "author": "Edvin Eriksson <edvinerikson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edvinerikson/relay-subscriptions/issues"
  },
  "homepage": "https://github.com/edvinerikson/relay-subscriptions#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^13.0.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.30.0",
    "jest-cli": "^13.0.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-relay": "^0.9.1",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "react-relay": "^0.9.1",
    "react": "^15.1.0"
  },
  "dependencies": {
    "invariant": "^2.2.1",
    "warning": "^3.0.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/core-js/",
      "<rootDir>/node_modules/react-relay/",
      "<rootDir>/node_modules/invariant/",
      "<rootDir>/node_modules/warning/",
      "<rootDir>/node_modules/enzyme"
    ]
  }
}
